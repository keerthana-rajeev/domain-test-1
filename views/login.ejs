<%- include("header") -%>
<body>
    <div class="container container1">
        <h3>Welcome To Domain Test</h3><br>
        <form method="POST" action="/">
            <div class="form-group">
              <label for="email">Email address:</label>
              <input type="email" class="form-control" placeholder="Enter email" id="email" name="username">
            </div>
            <div class="form-group">
              <label for="pwd">Registration Number:</label>
              <input type="password" class="form-control" placeholder="Enter registration number" id="pwd" name="password">
            </div>
            <div class="form-group">
              <label>
                <p>Not A User?&nbsp;<a href="/signup">Sign Up Now</a></p>
              </label>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>
    </div>
    <%- include("footer") -%>